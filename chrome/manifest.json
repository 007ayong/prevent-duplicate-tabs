{
    "name": "Prevent Duplicate Tabs",
    "version": "0.2.0",
    "manifest_version": 2,
    "description": "Prevent duplicate tabs",
    "icons": {
        "16":  "images/icon.png"
    },
    "browser_action": {
        "default_icon":  "images/icon.png",
        "default_popup": "views/popup.html"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "tabs"
    ]
}
